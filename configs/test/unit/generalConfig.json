{
  "rcplane": {
    "common": {
      "handshake_termination_string": "flush\r\n"
    },
    "io": {
      "journal": {
        "severity": "trace",
        "log_destination": "./logs/"
      },
      "serial_controller": {
        "tty_dev": "/tmp/rcplane_test_tty_dev_read",
        "baud_rate": 115200,
        "read_timeout_ms": 200,
        "write_timeout_ms": 200
      }
    },
    "autopilot": {
      "max_throttle": 255,
      "max_aileron_deflection": 35,
      "max_elevator_deflection": 40,
      "max_rudder_deflection": 20,
      "stabilize": {
        "kp": 0.8,
        "ki": 0.05,
        "kd": 0.02,
        "max_integral_error": 4
      }
    },
    "som": {
      "som_controller": {
        "handshake_attempts": 5,
        "main_loop_delay_ms": 20
      }
    }
  }
}